}
\examples{
eqO2.1 <- Eq.Ox.conc(temp.C = 20, elevation.m = 1000)

eqO2.2 <- Eq.Ox.conc(temp.C = 20,
bar.press = 674.1, bar.units = "mmHg")

eqO2.1
eqO2.2

}
